{
  "name": "fundmind",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "lint": "eslint --no-error-on-unmatched-pattern \"app/**/*.{ts,tsx}\" \"__tests__/**/*.{ts,tsx}\"",
    "test:ci": "jest --ci --config jest.config.js --passWithNoTests",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "typegen": "expo-router typegen",
    "test:run": "npm run test",
    "check:all": "npm run lint && npm run typecheck && npm run test:ci",
    "coverage": "jest --coverage",
    "test": "jest --passWithNoTests",
    "test:fix": "powershell -NoProfile -ExecutionPolicy Bypass -File ./tools/fix-index-and-test.ps1",
    "start:lan": "expo start --clear --host lan",
    "prepare": "husky",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo prebuild && expo run:android --variant release",
    "format": "prettier -w .",
    "commit": "cz"
  },
  "dependencies": {
    "@babel/runtime": "7.24.7",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-picker/picker": "2.11.1",
    "dayjs": "^1.11.18",
    "expo": "~54.0.0",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-localization": "~17.0.7",
    "expo-router": "~6.0.8",
    "expo-status-bar": "~3.0.8",
    "i18next": "^25.5.3",
    "lucide-react-native": "^0.544.0",
    "moti": "^0.30.0",
    "react": "19.1.0",
    "react-i18next": "^16.0.0",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-swiper": "^1.6.0",
    "react-native-worklets": "~0.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@expo/cli": "^0.18.17",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.4",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "~54.0.0",
    "commitizen": "^4.3.1",
    "cz-git": "^1.12.0",
    "eslint": "^9.37.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.12",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-test-renderer": "19.1.0",
    "simple-git-hooks": "^2.13.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.9.2",
    "whatwg-fetch": "^3.6.20"
  },
  "overrides": {
    "@babel/runtime": "7.24.7",
    "react": "19.1.0",
    "react-test-renderer": "19.1.0"
  },
  "eslintConfig": {
    "extends": [
      "expo"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npm run -s typecheck && npm test --silent"
  }
}